<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="mobile.extraado.com">

    <application
        android:allowBackup="true"
        android:icon="@drawable/logobackground"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logobackground"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Slides"></activity>
        <activity android:name=".SplashScreen" />
        <activity android:name=".NotificationEnabler" />
        <activity android:name=".MainActivity" />
        <activity android:name=".LocationServiceEnabler">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>