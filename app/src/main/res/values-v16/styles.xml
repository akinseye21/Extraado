<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">

    <style name="tab_text" parent="@android:style/TextAppearance.Widget.TabWidget">
        <item name="android:fontFamily">@font/poppinsregular</item>
        <item name="android:textSize">8dp</item>
    </style>

    <style name="tab_text2" parent="@android:style/TextAppearance.Widget.TabWidget">
        <item name="android:fontFamily" tools:targetApi="jelly_bean">@font/poppinsregular</item>
        <item name="android:textSize">8dp</item>
    </style>

    <style name="roundedImageView" parent="">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">20dp</item>
    </style>

    <style name="poppings_item" parent="android:Widget.TextView">
        <item name="android:fontFamily">@font/poppinsregular</item>
        <item name="android:textSize">12dp</item>
    </style>

    <style name="WindowAnimationTransition">
        <item name="android:windowEnterAnimation">@android:anim/fade_in</item>
        <item name="android:windowExitAnimation">@android:anim/fade_out</item>
        <item name="android:duration">5000</item>
    </style>

</resources>